page{
  margin: 0;
  padding:0;
  width:100%;
  height:100%;
  overflow: hidden;
  position: relative;
  overflow-x: hidden; 
  
}

.container{
 width: 100%;
 height: auto;
 margin: 0;
 padding:0;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.bg{
 width: 100%;
 height: 100%;
}

.header{
  width:100%;
  
}
.header .sign{
  width: 90%;
  height: 90%;
  position: absolute;
  left:5%;
  top:-8%; 
  
}
.btn{
  width: 500rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: absolute;
  top:60%;
}
